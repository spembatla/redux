const initialState = {
  isAddedToCart: false,
};

const addToCartReducer = (state = initialState, action) => {
  switch (action.type) {
    case "ADD_TO_CART_SUCCESS":
      state.isAddedToCart = !state.isAddedToCart;
      return state;
    default:
      return state;
  }
};

export default addToCartReducer;
